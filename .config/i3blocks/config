command=/usr/lib/i3blocks/$BLOCK_NAME
separator_block_width=45
markup=none

[empty]
command=echo " "
interval=once

[user]
label= 
command=echo $USER
interval=once

[wireless]
label=
command=/home/sogomn/Scripts/get_wifi.sh
interval=10

[volume]
label= 
command=/home/sogomn/Scripts/get_volume.sh
interval=once
signal=1
color=#33BBFF

[brightness]
label= 
command=cat "/sys/class/backlight/radeon_bl0/brightness"
interval=once
signal=2
color=#33BBFF

[battery]
label= 
command=/home/sogomn/Scripts/get_battery.sh
interval=15

[date]
label= 
command=date "+%d. %m. %Y"
interval=300

[time]
label= 
command=date "+%H:%M   "
interval=60
